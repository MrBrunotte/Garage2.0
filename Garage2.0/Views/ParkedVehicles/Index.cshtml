@model Garage2._0.Models. VehicleTypeViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

    <form asp-action="Filter">
        <div class="input-group mb-3">
            <input type="text" class="form-control" name="title" placeholder="Search..." />
            <div class="input-group-append">
                <select name="vehicleType" asp-items="@Html.GetEnumSelectList<VehicleType>()" class="form-control">
                    <option selected="selected" value="">Please select</option>
                </select>
            </div>
            <div class="input-group-append">
                <button type="submit" class="btn btn-primary"><i class="fa fa-search">&emsp;Search</i></button>
            </div>
        </div>
    </form>

    <partial name="ParkedVehicleListPartial" model="@Model.VehicleList" />

